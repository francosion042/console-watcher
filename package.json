{
  "name": "console-watcher",
  "version": "0.0.1",
  "description": "A NodeJs development tool that intercepts your app's “console.log()” statements, stores them for you, and lets you visualize them on your personal dashboard or a log file in your app directory.",
  "main": "lib/index.js",
  "types": "lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "run-example": "ts-node examples/example.ts",
    "test": "npm run test",
    "format": "prettier --write .",
    "update-version": "npm version 0.0.1 --no-git-tag-version && git add -A package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francosion042/console-watcher.git"
  },
  "keywords": [
    "NodeJs",
    "npm",
    "logging"
  ],
  "author": "Anthony Nwobodo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/francosion042/console-watcher/issues"
  },
  "homepage": "https://github.com/francosion042/console-watcher#readme",
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
